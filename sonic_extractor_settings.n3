@prefix xsd:      <http://www.w3.org/2001/XMLSchema#> .
@prefix vamp:     <http://purl.org/ontology/vamp/> .
@prefix :         <#> .

:transform a vamp:Transform ;
    vamp:plugin <http://vamp-plugins.org/rdf/plugins/vamp-libxtract#noisiness> ;
    vamp:step_size "256"^^xsd:int ;
    vamp:block_size "1024"^^xsd:int ;
    vamp:plugin_version """4""" ; 
    vamp:parameter_binding [
        vamp:parameter [ vamp:identifier "harmonic-threshold" ] ;
        vamp:value "0.1"^^xsd:float ;
    ] ;
    vamp:parameter_binding [
        vamp:parameter [ vamp:identifier "peak-threshold" ] ;
        vamp:value "10"^^xsd:float ;
    ] ;
    vamp:output <http://vamp-plugins.org/rdf/plugins/vamp-libxtract#noisiness_output_noisiness> .
